    "dataset:seed:latam": "node tools/experiments/intent-dataset-generate-latam.mjs",
    "dataset:seed:domains": "node tools/experiments/intent-dataset-generate-domain-variants.mjs",
    "dataset:seed:hardcases": "node tools/experiments/intent-dataset-generate-hardcases.mjs",
    "dataset:from:audit": "node tools/experiments/intent-dataset-from-audit.mjs",
    "dataset:from:last20": "node tools/experiments/intent-dataset-from-last20.mjs",
    "dataset:prepare": "node tools/experiments/intent-dataset-prepare.mjs",
    "dataset:eval:holdout": "node tools/experiments/intent-dataset-eval-holdout.mjs",
    "dataset:report:accuracy": "node tools/experiments/intent-accuracy-report.mjs",
    "dataset:report:accuracy:non-lim": "node tools/experiments/intent-accuracy-report.mjs --target=0.9 --exclude=lim",
    "dataset:report:domains": "node tools/experiments/intent-domain-report.mjs",
    "dataset:seed:domain-actions": "node tools/experiments/domain-action-intents-generate.mjs",
