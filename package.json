{
  "name": "houdi-agent",
  "version": "0.63b",
  "author": "Nazareno Tomaselli & Vrand",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
