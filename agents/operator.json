{
  "name": "operator",
  "description": "Agente operativo con permisos administrativos (equivalente a admin)",
  "cwd": "./workspace",
  "workspaceOnly": true,
  "allowCommands": [
    "pwd",
    "ls",
    "whoami",
    "date",
    "uptime",
    "uname",
    "df",
    "free",
    "ps",
    "rg",
    "cat",
    "head",
    "tail",
    "gmail-api",
    "sudo",
    "systemctl",
    "shutdown",
    "journalctl",
    "*"
  ]
}
